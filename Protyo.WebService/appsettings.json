{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "Database": {
     "AccessKeyId": "${access_key_id}",
     "SecretAccessKey": "${secret_access_key}",
     "RefreshTimer": "${database_refresh_timer}",
  },

  "MongoSettings": {
    "Url": "${mongo_url}",
  },

  "GoogleAppSettings": {
     "SpreadsheetId": "${spread_sheet_id}",
     "SheetName": "${sheet_name}",
     "Values": "${values}",
     "RefreshTimer": "${google_refresh_timer}"
  },


  "StripeConfigurationSettings": {
    "PublishableKey": "${stripe_publishable_key}",
    "SecretKey": "${stripe_secret_key}",
    "WebhookSecret": "${stripe_webhook_secret}",
    "Domain": "${stripe_domain}"
  },


  "CorsConfigurationSettings": {
    "Server2Server": [ "*" ]
  },

  "ApiSuccessRedirectUri": "${api_success_redirect_uri}",
  "ApiCanceledRedirectUri": "${api_cancel_redirect_uri}",
  "WebAccessToken": "${WebAccessToken}",
  "AllowedHosts": "*"
}
